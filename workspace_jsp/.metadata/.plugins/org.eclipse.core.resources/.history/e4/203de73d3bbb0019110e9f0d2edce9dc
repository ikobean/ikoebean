package com.b_board;

import java.util.List;
import java.util.Map;

import org.apache.log4j.Logger;

import com.vo.BoardMasterVO;

public class BoardLogic2 {
	Logger logger = Logger.getLogger(BoardLogic2.class);
	BoardDao2 bDao = null;
	
	public BoardLogic2() {
		bDao = new BoardDao2();
	}
	public List<Map<String, Object>> b_boardList(BoardMasterVO bmVO) {
		logger.info("b_boardList 호출 성공");
		List<Map<String,Object>> b_boardList=null;
		b_boardList = bDao.boardList(bmVO);
		return b_boardList;
	}

}
